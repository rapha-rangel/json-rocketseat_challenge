{
    "version": 2,
     
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "db.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
        }
    ]
}
